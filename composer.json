{
    "name": "collabora/online",
    "type": "drupal-module",
    "description": "Collabora Online connector for Drupal",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Hubert FiguiÃ¨re",
            "email": "hub@collabora.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "drupal/jwt": "^2.0"
    },
    "require-dev": {
        "composer/installers": "^1 || ^2",
        "drupal/core-composer-scaffold": "^10 | ^11@dev",
        "cweagans/composer-patches": "~1.0",
        "drupal/core-recommended": "^10.3.6 | ^11.0.5",
        "drupal/core-dev": "^10 | ^11",
        "openeuropa/task-runner-drupal-project-symlink": "^1.0.0-beta6",
        "php-parallel-lint/php-parallel-lint": "^1.2"
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/modules/contrib/{$name}": ["type:drupal-module"]
        },
        "patches": {
            "drupal/jwt": {
                "JwtTranscoder::$algorithmType must not be accessed before initialization (https://www.drupal.org/project/jwt/issues/3409096)": "patches/jwt-3409096.diff"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
